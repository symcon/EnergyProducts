                {
                    "Language": "de",
                    "Name": "Leistung, 3-phasig angeschlossen (Soll)"
                }
            ],
            "VariableType": 1,
            "WriteAction": "return [\n    \"SCN_Max_Current_per_Phase_L1\" => floor($VALUE/230/3),\n    \"SCN_Max_Current_per_Phase_L2\" => floor($VALUE/230/3),\n    \"SCN_Max_Current_per_Phase_L3\" => $VALUE/230 - (floor($VALUE/230/3) * 2),\n];"
