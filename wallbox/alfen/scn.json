            "Ident": "POWER_TARGET_1PHASED",
            "Name": "Power, 1-phased configuration (Target)",
            "Profile": "ALFEN.POWER_TARGET_1PHASED",
            "ReadAction": "return ($VALUES[\"SCN_Max_Current_per_Phase_L1\"]+$VALUES[\"SCN_Max_Current_per_Phase_L2\"]+$VALUES[\"SCN_Max_Current_per_Phase_L3\"])*230;",
            "Translation": [
                {
                    "Language": "de",
                    "Name": "Leistung, 1-phasig angeschlossen (Soll)"
                }
            ],
            "VariableType": 1,
            "WriteAction": "return [\n    \"SCN_Max_Current_per_Phase_L1\" => $VALUE/230,\n    \"SCN_Max_Current_per_Phase_L2\" => $VALUE/230,\n    \"SCN_Max_Current_per_Phase_L3\" => $VALUE/230,\n];"
        },
        {
            "Active": true,
            "Ident": "POWER_TARGET_3PHASED",
            "Name": "Power, 3-phased configuration (Target)",
            "Profile": "ALFEN.POWER_TARGET_3PHASED",
            "ReadAction": "return ($VALUES[\"SCN_Max_Current_per_Phase_L1\"]+$VALUES[\"SCN_Max_Current_per_Phase_L2\"]+$VALUES[\"SCN_Max_Current_per_Phase_L3\"])*230;",
